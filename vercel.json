{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "^/$", "dest": "/api/index.php" },
    { "src": "^/login\\.php$", "dest": "/api/login.php" },
    { "src": "^/register\\.php$", "dest": "/api/register.php" },
    { "src": "^/logout\\.php$", "dest": "/api/logout.php" },
    { "src": "^/account\\.php$", "dest": "/api/account.php" },
    { "src": "^/admin\\.php$", "dest": "/api/admin.php" },
    { "src": "^/content_free\\.php$", "dest": "/api/content_free.php" },
    { "src": "^/content_premium\\.php$", "dest": "/api/content_premium.php" }
  ]
}
